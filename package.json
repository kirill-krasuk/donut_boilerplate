	"scripts": {
		"bundle": "bash ./tools/build_app.sh",
		"ci:test": "bash ./tools/ci/ci.sh",
		"ci:lint": "bash ./tools/ci/lint.sh",
		"lint": "eslint .",
		"lint-fix": "eslint --fix .",
		"lint:styled": "stylelint --config ./config/css/styled.stylelint.json './src/**/styles/*.{ts,js,jsx,tsx}'",
		"lint:css": "stylelint --config ./config/css/css.stylelint.json './src/**/*.{css,sass,scss}'",
		"lint:css-fix": "stylelint --config ./config/css/css.stylelint.json './src/**/*.{css,sass,scss}' --fix",
		"format:code": "prettier --config ./.prettierrc --write '**/*.{js,jsx,ts,tsx}' && eslint --fix .",
		"prunecaches": "bash ./tools/common/prune_caches.sh",
		"run:server": "bash ./tools/run_server.sh",
		"start": "bash ./tools/start.sh",
		"start:prod": "bash ./tools/start_prod.sh",
		"storybook": "start-storybook -c ./config/.storybook",
		"storybook:build": "build-storybook -c ./config/.storybook",
		"generate": "bash ./tools/lib/generators/generate.sh",
		"gen": "yarn generate",
		"test": "bash ./tools/ci/test.sh",
		"test:coverage": "bash ./tools/ci/test.sh c",
		"test:update": "bash ./tools/ci/test.sh u",
		"test:watch": "bash ./tools/ci/test.sh w",
		"types": "tsc --noEmit -p .",
		"prepare": "husky install",
		"pre-commit": "yarn lint-staged",
		"commit": "cz",
		"release": "standard-version"
	},
