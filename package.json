  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "start:app": "cross-env BABEL_ENV=development NODE_ENV=development node ./dist/server.js",
    "build:server": "webpack --config ./webpack/webpack.server.js --mode=development",
    "start": "npm run build:server && npm run start:app",
    "run:prod": "cross-env NODE_ENV=production node ./dist/server.js",
    "build:dll": "webpack --config ./webpack/webpack.dll.js",
    "build:prod:server": "webpack --config ./webpack/webpack.server.js --mode=production",
    "build:prod:client": "BABEL_ENV=production webpack --config ./webpack/webpack.prod.js --mode=production",
    "build": "webpack --config ./webpack/webpack.server.js --mode=production",
    "start:prod": "npm i && rm -rf .cache && npm run build:prod:server && npm run build:prod:client && npm run run:prod",
    "prunecaches": "rm -rf ./.cache",
    "postinstall": "npm run prunecaches",
    "postuninstall": "npm run prunecaches",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lint": "eslint .",
    "lint:css": "stylelint './src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-types": "tsc --noEmit -p .",
    "create:component": "node ./cli/scripts/create-component.js"
  },
