[ignore]
<PROJECT_ROOT>/flow-typed/.*
<PROJECT_ROOT>/node_modules/config-chain/test/.*
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/webpack/.*
<PROJECT_ROOT>/internals/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/server/.*

[include]

[libs]
./lib-defs/

[lints]

[options]
suppress_comment=^ flow-disable-next-line
module.name_mapper='^core' -> '<PROJECT_ROOT>/src/core'
module.name_mapper='^app' -> '<PROJECT_ROOT>/src/app'
module.name_mapper='^server' -> '<PROJECT_ROOT>/server'
module.name_mapper='^public' -> '<PROJECT_ROOT>/public'
module.name_mapper='^images' -> '<PROJECT_ROOT>/src/assets/images'
module.name_mapper='^svgs' -> '<PROJECT_ROOT>/src/assets/svgs'
module.name_mapper='^ui' -> '<PROJECT_ROOT>/src/ui'
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.sass
esproposal.decorators=ignore

[strict]
